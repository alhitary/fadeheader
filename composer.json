{
    "name": "sanfi/fadeheader",
    "type": "phpbb-extension",
    "description": "Images rotation in header with transition effect for forum PHPBB 3.1.X",
    "homepage": "http://www.microcosmoacquari.it/forum/",
    "version": "1.0.5",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "sanfi",
            "email": "sanfi@microcosmoacquari.it",
            "homepage": "http://www.microcosmoacquari.it",
            "role": "Developer"
		},
		{
			"name": "Sheer",
			"homepage": "http://www.phpbbguru.net/",
			"role": "Support Team"
		}],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Fade Header",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/fade_header",
            "filename": "version_check"
        }
    }
}
